    "@unocss/preset-uno": "^0.56.5",
    "esbuild": "^0.19.4",
    "fs-extra": "^11.1.1",
    "jiti": "^1.20.0",
    "pnpm": "^8.9.2",
    "tslib": "^2.6.2",
    "unocss": "^0.56.5"
  },
  "dependencies": {
    "@solid-primitives/event-bus": "^1.0.8",
    "@solid-primitives/scheduled": "^1.4.1",
    "@solid-primitives/storage": "^2.1.1",
